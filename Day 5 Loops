import math
import os
import random
import re
import sys



if __name__ == '__main__':
  """Using a for loop after the input"""
    n = int(input())
    for a in range(1, 11):
        print(f"{n} x {a} = {n*a}")

# Alternatively using list comprehensions
# the same for loop and print funtion is used
# the syntax for a list comprehension is 
# the expression that you want evaluated followed by a for clause/or loop, then an if clause if any
# this is enveloped as a list and assigned to a variable
c = [print(f"{n} x {a} = {n*a}") for a in range(1, 11)]
